export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME="agnoster"
 ENABLE_CORRECTION="true"
plugins=(zsh-autosuggestions autojump git)
source $ZSH/oh-my-zsh.sh
alias v='nvim'
alias ga='git add'
alias gc='git commit -m'
alias gp='git push'
alias gco='git checkout'
alias gs='git status'
alias gf='git fetch'
alias gb='git branch'
alias gsw='git switch'
alias c='clear'
alias la='ls -a'
alias lt='tree'
alias fnd='sudo find / -name'
alias sndrst='systemctl --user restart pipewire && systemctl --user restart pipewire-pulse && systemctl --user restart pipewire.socket && systemctl --user restart pipewire.service'
alias ..='cd ..'
alias ...='cd ../..'
alias update='sudo dnf update && sudo dnf upgrade'
alias install='sudo dnf install'
alias uninstall='sudo dnf remove'
alias pkglst='sudo dnf list installed'
alias pkglstf='sudo dnf list installed | grep '
alias h='cd ~'
alias fl-pug='flutter pub get'
alias fl-pua='flutter pub add'
alias fl-puu='flutter pub upgrade'
alias fl-r='flutter run'
alias fl-ds='flutter devices'
alias fl-ems='flutter emulators'
alias fl-emr='flutter emulators --launch Medium_Phone_API_35'
alias fl-cln='flutter clean'
alias fl-anal='flutter analyze'
alias fl-remnra='flutter emulators --launch Medium_Phone_API_35 && flutter run'
alias fl-doc='flutter doctor'
